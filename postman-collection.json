        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@rariteti.rs\",\n  \"password\": \"CHANGE_ME_ON_FIRST_LOGIN\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/admin/auth/login",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "admin",
                "auth",
                "login"
              ]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "  pm.collectionVariables.set('token', pm.response.json().access_token);",
                  "}"
                ]
              }
            }
          ]
        }
      ]
